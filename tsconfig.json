{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": false,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/auth/callback/route.js",
    "src/login/page.js",
    "src/api/redirect.js",
    "src/components/Dashboard.jsx",
    "src/components/Dashboard.jsx",
    "src/app/dashboard/page.jsx",
    "src/app/signup/page.jsx",
    "src/components/store.jsx"
  ],
  "exclude": ["node_modules"]
}
